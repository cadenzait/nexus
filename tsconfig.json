{
  "include": [
    "./src/**/*"
  ],
  "exclude": [
  ],
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "target": "ES6",
    "typeRoots": [
      "node_modules/@types",
      "typings"
    ],
    "lib": [
      "es2016",
      "es2017"
    ]
  }
}